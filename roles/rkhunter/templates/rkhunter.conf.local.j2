ALLOWIPCPROC=/usr/bin/dunst
ALLOWIPCPROC=/usr/lib/firefox/firefox
ALLOWHIDDENDIR=/etc/.duplicacy
ALLOWHIDDENFILE=/etc/.updated
ALLOWHIDDENFILE=/usr/share/man/man5/.k5identity.5.gz
